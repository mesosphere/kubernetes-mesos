From 3ffb6193b5152fc3a2e2a4a12111130f4b531110 Mon Sep 17 00:00:00 2001
From: Brendan Burns <bburns@google.com>
Date: Tue, 7 Apr 2015 20:03:29 -0700
Subject: [PATCH] Kubernetes version v0.14.2

---
 pkg/version/base.go | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/pkg/version/base.go b/pkg/version/base.go
index 61e5004..442f2b4 100644
--- a/pkg/version/base.go
+++ b/pkg/version/base.go
@@ -36,8 +36,8 @@ package version
 var (
 	// TODO: Deprecate gitMajor and gitMinor, use only gitVersion instead.
 	gitMajor     string = "0"              // major version, always numeric
-	gitMinor     string = "14.0"           // minor version, numeric possibly followed by "+"
-	gitVersion   string = "v0.14.0"        // version from git, output of $(git describe)
+	gitMinor     string = "14.2"           // minor version, numeric possibly followed by "+"
+	gitVersion   string = "v0.14.2"        // version from git, output of $(git describe)
 	gitCommit    string = ""               // sha1 from git, output of $(git rev-parse HEAD)
 	gitTreeState string = "not a git tree" // state of git tree, either "clean" or "dirty"
 )
