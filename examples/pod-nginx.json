{
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "id": "nginx-id-01",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "containers": [{
        "name": "nginx-01",
        "image": "library/nginx",
        "ports": [{
          "containerPort": 80,
          "name": "http"
        }],
        "livenessProbe": {
          "enabled": true,
          "type": "http",
          "initialDelaySeconds": 30,
          "httpGet": {
            "path": "/",
            "port": "80"
          }
        }
      }]
    }
  },
  "labels": {
    "name": "nginx",
    "cluster": "gce"
  }
}
